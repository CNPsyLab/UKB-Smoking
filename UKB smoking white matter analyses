library(data.table)
library(psych)
library(moments)
library(mice)

#Load 4/2019 imaging covar
img_cov <- read.table("FILEPATH", header=TRUE, sep="\t")
#takes approximately 2 minutes to read in
#started at 10:02am - finished by 10:04am
head(img_cov)
dim(img_cov) 

##### Potentially Important Confounds #####
# 25756	Scanner lateral (X) brain position ##
# 25757	Scanner transverse (Y) brain position ##
# 25758	Scanner longitudinal (Z) brain position ##
# 25759	Scanner table position # not using for now

##Additionals (JCG)
# 25008-2.0 Volume of white matter

###Load in DTI data
#Load 4/2019 imaging covar
DTIdata <- read.table("FILEPATH", header=TRUE, sep="\t")
#takes approximately 2 minutes to read in
#started at 10:02am - finished by 10:04am
head(DTIdata)
dim(DTIdata) 
nrow(DTIdata[complete.cases(DTIdata),]) 

##Merge imaging covar w/ DTI data
all_img <- merge(img_cov,DTIdata,by="f.eid")

