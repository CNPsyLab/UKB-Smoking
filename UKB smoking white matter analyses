##########################
####SMOKING SECTION#######
#current smoking
summary(all_pheno$f.1239.2.0)
table(all_pheno$f.1239.2.0)
#https://biobank.ctsu.ox.ac.uk/crystal/field.cgi?id=1239
#1? = yes, on most or all days (39,250)
#2? = only occasionally (13,735)
#0 = no (448,310)
#-3 prefer not to answer? (429)

#f.31.0.0 = sex
# f.34.0.0            year of birth
# f.52.0.0            month of birth
# 3456-2.0 n cigs currently smoked daily (current cigarette smokers)
# 2887-2.0 n cigs previously smoked daily (past manufactured/hand rolled cigarettes)
# 6183-2.0 n cigs previously smoked daily (current cigar/pipe smokers)
# 1239-2.0 n current tobacco smoking (prefer not to answer, no, yes, on most days, only occasionally)

pheno_subset <- all_pheno[,c("f.eid","f.31.0.0", "f.738.2.0", "f.21000.0.0","f.2644.2.0", "f.1239.0.0", "f.1249.2.0", "f.1239.2.0", "f.6183.2.0",
				"f.3456.2.0", "f.2887.2.0", "f.3436.2.0","f.2867.2.0", "f.5959.2.0", "f.2877.2.0", "f.2897.2.0", "f.21001.2.0", "f.1558.2.0",
				 "f.4440.2.0", "f.4407.2.0", "f.4451.2.0", "f.4418.2.0", "f.4429.2.0", "f.4462.2.0", 
				"f.1598.2.0", "f.1568.2.0", "f.1608.2.0", "f.1578.2.0", "f.1588.2.0", "f.5364.2.0", "f.48.2.0", "f.49.2.0")]
myData <- merge(pheno_subset,T1_subset,by="f.eid")
nrow(myData)

myData <- merge(myData,new_covar,by="f.eid")
nrow(myData)

##complete cases of white matter data##
myData <- myData[complete.cases(myData$f.25008.2.0),] #WM Vol complete cases for neuroimaging data (Pheno comes later)
nrow(myData) #21407
myData <- myData[complete.cases(myData$f.25488.2.0),] # acoustic radiation complete cases for neuroimaging data (Pheno comes later)
nrow(myData) #19395
myData <- myData[complete.cases(myData$f.25781.2.0),] #drop 43 missing WMH volume
nrow(myData)

####RECODE ETHNICITY VARIABLE 0 = WHITE, 1 = NON-WHITE####
myData$f.21000.0.0[myData$f.21000.0.0==1001] <- 1
myData$f.21000.0.0[myData$f.21000.0.0==1002] <- 1
myData$f.21000.0.0[myData$f.21000.0.0==1003] <- 1
myData$f.21000.0.0[myData$f.21000.0.0==1] <- 0
myData$f.21000.0.0[myData$f.21000.0.0==3] <- 2
myData$f.21000.0.0[myData$f.21000.0.0==4] <- 2
myData$f.21000.0.0[myData$f.21000.0.0==5] <- 2
myData$f.21000.0.0[myData$f.21000.0.0==6] <- 2
myData$f.21000.0.0[myData$f.21000.0.0>1003] <- 2
myData$f.21000.0.0[myData$f.21000.0.0==2] <- 1

###exclude missing 
myData <- myData[complete.cases(myData$f.21000.0.0),]
#Remove do not know/prefer not to answer
myData <- myData[!myData$f.21000.0.0 < 0,]
nrow(myData) #58

##
myData <- myData[complete.cases(myData$f.1239.2.0),] #drop 134 who did not answer first smoking question
nrow(myData)

#### EXCLUDE CIGAR SMOKERS THAT NEVER SMOKED HAND-ROLLED OR MANUFACTURED CIGS ON MOST DAYS (b/c hard to know their exposure) ###
myData <- myData[!grepl(-3, myData$f.5959.2.0),] #0 (or -3 prefer not to answer) on # 5959.2.0 Previously smoked cigarettes on most/all days (triggered by answering cigars to 3446-2.0)
myData <- myData[!grepl(0, myData$f.5959.2.0),] 
myData <- myData[!grepl(-3, myData$f.1239.2.0),] #excluding for prefer not to answer
myData <- myData[!grepl(-3, myData$f.2877.2.0),]
myData$noncurrentsmoker[myData$f.1239.2.0==1] <- 1
myData$noncurrentsmoker[myData$f.1239.2.0==0] <- 1
myData$typeprev[myData$f.2877.2.0==3] <- 1
myData$typeprev[myData$f.2877.2.0==-7] <- 1
myData$cigartoexclude <- myData$noncurrentsmoker + myData$typeprev
myData <- myData[!grepl(2, myData$cigartoexclude),] 
myData$noncurrentsmoker <- NULL
myData$typeprev <- NULL
myData$cigartoexclude <- NULL

##### Create smoke consumption Lifetime (INCLUDING PREVIOUS CPD FOR CURRENT CIGAR SMOKERS) #####
myData$cons_lt <- ifelse(!is.na(myData$f.3456.2.0), myData$f.3456.2.0, ifelse(!is.na(myData$f.2887.2.0), myData$f.2887.2.0, myData$f.6183.2.0))
table(myData$cons_lt, useNA = "ifany") #
sum(table(myData$cons_lt))
sum(table(myData$cons_lt, useNA = "ifany")) #
myData <- myData[!grepl(-10, myData$cons_lt),] #Remove those who said less than 1 per day (-10) because they already said they smoke(d) on most or all days
myData <- myData[!grepl(-1, myData$cons_lt),] #remove -1 (I do not know)
myData$cons_lt[is.na(myData$cons_lt)] <- 0 #Recode NA to 0 so all subj are included in analyses
myData$cons_lt <- ifelse(myData$f.1239.2.0==1 & myData$cons_lt==0, NA, ifelse(myData$f.1249.2.0==1 & myData$cons_lt==0, NA, myData$cons_lt))  #REMOVE THOSE WHO SAID SMOKING (past or present) most or all days but have a 0 for consumption (b/c missing value)
myData <- myData[complete.cases(myData$cons_lt),] #drop these missing values

###################################################              
##### Calc age started smoking #####
myData <- myData[!grepl(-3, myData$f.3436.2.0),] #remove prefer not to answer
myData <- myData[!grepl(-3, myData$f.2867.2.0),] #remove prefer not to answer
myData <- myData[!grepl(-1, myData$f.3436.2.0),] #remove do not know
myData <- myData[!grepl(-1, myData$f.2867.2.0),] #remove do not know
myData$age_init <- ifelse(!is.na(myData$f.3436.2.0), myData$f.3436.2.0, myData$f.2867.2.0)
table(myData$age_init, useNA = "ifany") 
sum(table(myData$age_init)) 
sum(table(myData$age_init, useNA = "ifany")) 
myData <- myData[!grepl(-3, myData$age_init),] #remove prefer not to answer

#read in dataset of calc age
#AGE$age_at_asst3 (age at assessment 3, neuroimaging)
setwd("FILEPATH")
AGE <- fread("UKB_AGE_calc_v2.csv") #automatically detects header and separator
AGE <- AGE[,c("f.eid", "age_at_asst3")]

myData <- merge(myData,AGE,by="f.eid")
myData$age_at_asst3 <- as.numeric(myData$age_at_asst3)
myData <- myData[!grepl(-1, myData$f.2897.2.0),] #remove do not know
myData$duration <- ifelse(myData$f.1239.2.0==1, myData$age_at_asst3-myData$age_init, ifelse(myData$f.1249.2.0==1, myData$f.2897.2.0-myData$age_init, NA)) #for now this includes cigar smokers lumped in with cigarette smokers
#Field 1249 was collected from all participants except those who indicated they currently smoke on most or all days, as defined by their answers to Field 1239
table(myData$duration)
#^Note the 0s are likely from people who started and stopped smoking at the same age (myData$f.2897.2.0-myData$age_init); current decision is to drop these
myData <- myData[!grepl(0, myData$duration),]  
#^NOTE THIS IN THE METHODS MAYBE CHANGE MIND ON THIS..
myData$duration[is.na(myData$duration)] <- 0 #Recode NA to 0 so all subj are included in analyses

nrow(myData[complete.cases(age_at_asst3),])
nrow(myData)

##Calc time since quitting (all current smokers are 0, former are >0, and never should be missing??<- verify this is ok)
##NOTE I AM NOT USING f.6194.2.0 = current cigar smokers on most days that used to smoke cigarettes? "How old were you when you last smoked cigarettes on most days?"
#f.2897.2.0 = former smokers "How old were you when you last smoked on most days?"
table(myData$f.2897.2.0)
myData$f.2897.2.0[is.na(myData$f.2897.2.0)] <- 0 #recode NA to 0 
myData$timesinceq <- ifelse(myData$f.2897.2.0>0, myData$age_at_asst3-myData$f.2897.2.0, 0)
summary(myData$timesinceq)
#Note after removing never smokers, the distribution is good

###Excluding for missing primary DVs###
myData <- myData[complete.cases(myData$cons_lt),]
myData <- myData[complete.cases(myData$duration),]

#drop subj ID
myData <- subset(myData, select = -c(f.eid))

########Extra data cleaning#########
######CALC ALCOHOL CONSUMPTION MONTHLY AS COVARIATE######
table(myData$f.1558.2.0, useNA = "ifany")
sum(is.na(myData$f.1558.2.0)) 

###change prefer not to answer/don't knows to missing
myData$f.1558.2.0[myData$f.1558.2.0<0] <- NA
myData$f.4440.2.0[myData$f.4440.2.0<0] <- NA
myData$f.4407.2.0[myData$f.4407.2.0<0] <- NA
myData$f.4451.2.0[myData$f.4451.2.0<0] <- NA
myData$f.4418.2.0[myData$f.4418.2.0<0] <- NA
myData$f.4429.2.0[myData$f.4429.2.0<0] <- NA
myData$f.4462.2.0[myData$f.4462.2.0<0] <- NA
myData$f.1598.2.0[myData$f.1598.2.0<0] <- NA
myData$f.1568.2.0[myData$f.1568.2.0<0] <- NA
myData$f.1608.2.0[myData$f.1608.2.0<0] <- NA
myData$f.1578.2.0[myData$f.1578.2.0<0] <- NA
myData$f.1588.2.0[myData$f.1588.2.0<0] <- NA
myData$f.5364.2.0[myData$f.5364.2.0<0] <- NA

#Recode frequency in order of severity with never == 0
myData$f.1558.2.0[myData$f.1558.2.0==6] <- 0
myData$f.1558.2.0[myData$f.1558.2.0==1] <- 6
myData$f.1558.2.0[myData$f.1558.2.0==2] <- 5
myData$f.1558.2.0[myData$f.1558.2.0==3] <- 4
myData$f.1558.2.0[myData$f.1558.2.0==4] <- 3
myData$f.1558.2.0[myData$f.1558.2.0==5] <- 2

#Calculate total monthly consumption for session 2
myData$MonthlyTotal.2 <- rowSums(myData[,c("f.4440.2.0", "f.4407.2.0", "f.4451.2.0", "f.4418.2.0", "f.4429.2.0", "f.4462.2.0")])
table(myData$MonthlyTotal.2, useNA="ifany")

#Calculate total weekly consumption and convert to monthly for session 2
myData$WeeklyTotal.2 <- rowSums(myData[,c("f.1598.2.0", "f.1568.2.0", "f.1608.2.0", "f.1578.2.0", "f.1588.2.0", "f.5364.2.0")])
myData$WeeklyTotal.2 <- myData$WeeklyTotal.2*4
table(myData$MonthlyTotal.2, useNA="ifany")

#Calculate monthly alcohol consumption
myData$AlcCons <- ifelse(myData$f.1558.2.0==0,myData$f.1558.2.0,ifelse(!is.na(myData$MonthlyTotal.2), myData$MonthlyTotal.2, ifelse(!is.na(myData$WeeklyTotal.2), myData$WeeklyTotal.2, -111)))

#Recode
table(myData$AlcCons)
myData$AlcCons[myData$AlcCons<0] <- NA
table(myData$AlcCons)

#######BMI & WHRatio###########
table(myData$f.48.2.0) #waist circum
table(myData$f.49.2.0) #hip circum
sum(is.na(myData$f.48.2.0)) #note this is likely discrepant from total imaging numbers, usually it will have less missing than imaging because the imaging data takes longer for them to update/refresh
sum(is.na(myData$f.49.2.0)) 
sum(is.na(myData$f.21001.2.0)) #BMI for comparison

myData$WHRatio <- myData$f.48.2.0/myData$f.49.2.0

###Characterize current, ex, never smokers
summary(myData$f.1239.2.0)
summary(myData$f.1249.2.0)
table(myData$f.1239.2.0)
table(myData$f.1249.2.0)
table(myData$cons_lt)
nrow(myData)

####REMOVE IRRELEVANT COLUMNS
myData <- subset(myData, select = -c(f.1558.2.0, f.2644.2.0,f.1239.0.0, f.1249.2.0, f.1239.2.0,f.6183.2.0, f.3456.2.0, f.2887.2.0, f.3436.2.0,f.2867.2.0,f.5959.2.0,f.2877.2.0, f.2897.2.0, age_init, MonthlyTotal.2, WeeklyTotal.2, f.4440.2.0, f.4407.2.0, f.4451.2.0, f.4418.2.0, f.4429.2.0, f.4462.2.0,
		f.1598.2.0, f.1568.2.0, f.1608.2.0, f.1578.2.0, f.1588.2.0, f.5364.2.0, f.48.2.0, f.49.2.0))

#recode refuse to answer/do not know
summary(myData$f.738.2.0)
myData$f.738.2.0[myData$f.738.2.0<0] <- NA
summary(myData$f.738.2.0)

####PERCENT MISSING APPROACH ######
#Calculate percent missing rows
percentmiss = function(x) { sum(is.na(x)) / length(x) * 100 }
missingsubjects = apply(myData,1,percentmiss)
summary(missingsubjects)
table(missingsubjects)

#Calculate percent missing columns
apply(myData,2,percentmiss)

###IMPUTATION SECTION###
set.seed(103)
#first exclude predictors you don't want influencing imputation
init = mice(myData, maxit=0)
meth = init$method
predM = init$predictorMatrix
predM[, c("cons_lt","duration", "timesinceq","f.25008.2.0","f.25781.2.0","f.25756.2.0","f.25757.2.0","f.25758.2.0","f.25488.2.0",	"f.25489.2.0",	"f.25490.2.0",	"f.25491.2.0",	"f.25492.2.0",	"f.25493.2.0",	"f.25494.2.0",	"f.25495.2.0",	"f.25496.2.0",	"f.25497.2.0",	"f.25498.2.0",	"f.25499.2.0",	"f.25500.2.0",	"f.25501.2.0",	"f.25502.2.0",	"f.25503.2.0",	
"f.25504.2.0",	"f.25505.2.0",	"f.25506.2.0",	"f.25507.2.0",	"f.25508.2.0",	"f.25509.2.0",	"f.25510.2.0",	"f.25511.2.0",	"f.25512.2.0",	"f.25513.2.0",	"f.25514.2.0",	"f.25515.2.0",	"f.25516.2.0",	"f.25517.2.0",	"f.25518.2.0",	"f.25519.2.0",	
"f.25520.2.0",	"f.25521.2.0",	"f.25522.2.0",	"f.25523.2.0",	"f.25524.2.0",	"f.25525.2.0",	"f.25526.2.0",	"f.25527.2.0",	"f.25528.2.0",	"f.25529.2.0",	"f.25530.2.0",	"f.25531.2.0",	"f.25532.2.0",	"f.25533.2.0",	"f.25534.2.0",	"f.25535.2.0",	
"f.25536.2.0",	"f.25537.2.0",	"f.25538.2.0",	"f.25539.2.0",	"f.25540.2.0",	"f.25541.2.0")]=0

myData$f.31.0.0<-as.factor(myData$f.31.0.0)
myData$f.21000.0.0<-as.factor(myData$f.21000.0.0)
