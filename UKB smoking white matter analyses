###SUPPLEMENTAL - correlation matrix
###Cor matrix - all var
library(Hmisc)
flattenCorrMatrix <- function(cormat, pmat) {
  ut <- upper.tri(cormat)
  data.frame(
    row = rownames(cormat)[row(cormat)[ut]],
    column = rownames(cormat)[col(cormat)[ut]],
    cor  =(cormat)[ut],
    p = pmat[ut]
  )
}
res2<-rcorr(as.matrix(myData))
res3 <- flattenCorrMatrix(res2$r, res2$P)
fwrite(res3, "FILEPATH", na=".")

###Cor matrix - only covar and global measures
myData <- myData[,c("cons_lt","duration","age_at_asst3","f.31.0.0","f.21000.0.0", "f.21001.2.0", "WHRatio","SysBP","DiaBP","f.6138.2.0", "f.738.2.0", "AlcCons", "f.25756.2.0", "f.25757.2.0", "f.25758.2.0", "f.25781.2.0", "f.25008.2.0","timesinceq")] #drop irrel var
myData$smokingstatus <- myData$cons_lt
myData$smokingstatus[myData$smokingstatus>0] <- 1 #recode so its 1 = yes, 0 = no
flattenCorrMatrix <- function(cormat, pmat) {
  ut <- upper.tri(cormat)
  data.frame(
    row = rownames(cormat)[row(cormat)[ut]],
    column = rownames(cormat)[col(cormat)[ut]],
    cor  =(cormat)[ut],
    p = pmat[ut]
  )
}
res2<-rcorr(as.matrix(myData))
res3 <- flattenCorrMatrix(res2$r, res2$P)
fwrite(res3, "FILEPATH", na=".")

###conditional correlation coefficients
##duration, CPD, and timesinceq in current/former smoker only sample
myData$smokingstatus[myData$smokingstatus==0] <- NA
myData <- myData[complete.cases(myData$smokingstatus),]
flattenCorrMatrix <- function(cormat, pmat) {
  ut <- upper.tri(cormat)
  data.frame(
    row = rownames(cormat)[row(cormat)[ut]],
    column = rownames(cormat)[col(cormat)[ut]],
    cor  =(cormat)[ut],
    p = pmat[ut]
  )
}
res2<-rcorr(as.matrix(myData))
res3 <- flattenCorrMatrix(res2$r, res2$P)
fwrite(res3, "FILEPATH", na=".")







