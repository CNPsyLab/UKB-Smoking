##READ IN PHENO from 4/2019
#path from server
setwd("FILEPATH")
#I do not recommend loading this in via computer
#read in pheno dataset 
all_pheno <- fread("ukb29151.tab") #automatically detects header and separator

##READ IN NEW COVAR from 6/2019
setwd("FILEPATH")
new_covar <- fread("ukb32996.tab")
###Wrangle covar
#Education
table(new_covar$f.6138.2.0)
#-7 = none of the above; -3 = prefer not to answer; 1 = college/univ; 2 = A/AS levels; 3 = O levels/GCSE; 4 = CSE; 5= NVQ; 6 = Nursing/teaching;
#there are 6 columns, 0 contains 1-6 (and missing options), 1 contains 2-6, 2 contains 3-6, etc
#only interested in if have college/univ degree (1)
new_covar$f.6138.2.0[new_covar$f.6138.2.0==-3] <- NA
new_covar$f.6138.2.0[new_covar$f.6138.2.0==-7] <- 0
new_covar$f.6138.2.0[new_covar$f.6138.2.0>1] <- 0
#Disease code

#Blood pressure
#94 <-A manual sphygmometer was used if the standard automated device could not be employed.
#Average the two readings

new_covar$DiaBP <- ifelse(new_covar$f.4079.2.0 > 0 & new_covar$f.4079.2.1 > 0, (new_covar$f.4079.2.0 + new_covar$f.4079.2.1)/2, NA)
summary(new_covar$DiaBP)
new_covar$DiaBP <- ifelse(is.na(new_covar$DiaBP), new_covar$f.4079.2.0,new_covar$DiaBP)
summary(new_covar$DiaBP)
new_covar$DiaBP <- ifelse(is.na(new_covar$DiaBP), new_covar$f.4079.2.1,new_covar$DiaBP)
summary(new_covar$DiaBP)
new_covar$DiaBP <- ifelse(is.na(new_covar$DiaBP) & new_covar$f.94.2.0 > 0 & new_covar$f.94.2.1 > 0 , (new_covar$f.94.2.0 + new_covar$f.94.2.1)/2,new_covar$DiaBP)
summary(new_covar$DiaBP)
new_covar$DiaBP <- ifelse(is.na(new_covar$DiaBP), new_covar$f.94.2.0,new_covar$DiaBP)
summary(new_covar$DiaBP)
new_covar$DiaBP <- ifelse(is.na(new_covar$DiaBP), new_covar$f.94.2.1,new_covar$DiaBP)
summary(new_covar$DiaBP)

new_covar$SysBP <- ifelse(new_covar$f.4080.2.0 > 0 & new_covar$f.4080.2.1 > 0, (new_covar$f.4080.2.0 + new_covar$f.4080.2.1)/2, NA)
summary(new_covar$SysBP)
new_covar$SysBP <- ifelse(is.na(new_covar$SysBP), new_covar$f.4080.2.0,new_covar$SysBP)
summary(new_covar$SysBP)
new_covar$SysBP <- ifelse(is.na(new_covar$SysBP), new_covar$f.4080.2.1,new_covar$SysBP)
summary(new_covar$SysBP)
new_covar$SysBP <- ifelse(is.na(new_covar$SysBP) & new_covar$f.93.2.0 > 0 & new_covar$f.93.2.1 > 0, (new_covar$f.93.2.0 + new_covar$f.93.2.1)/2, new_covar$SysBP)
summary(new_covar$SysBP)
new_covar$SysBP <- ifelse(is.na(new_covar$SysBP), new_covar$f.93.2.0,new_covar$SysBP)
summary(new_covar$SysBP)
new_covar$SysBP <- ifelse(is.na(new_covar$SysBP), new_covar$f.93.2.1,new_covar$SysBP)
summary(new_covar$SysBP)

grep("20002", names(new_covar), value = TRUE) #check for every illness column

