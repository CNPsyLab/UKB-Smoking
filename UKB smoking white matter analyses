#impute
imp.myData <- mice(myData, method=meth, predictorMatrix=predM, mech = "MNAR") #only use MNAR if think values are not missing at random
imp.myData$meth
densityplot(imp.myData, ~f.21001.2.0) ##LOOK OUT FOR IMPOSSIBLE VALUES (e.g., negative BMI)
imp.myData <- complete(imp.myData)

myData <- imp.myData

